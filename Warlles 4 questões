#include<stdio.h>
#include <stdlib.h>
int main()

{
 int a, b;
    int soma, sub, multi, divi, resto;

    printf("Informe o a: ");
    scanf("%d", &a);

    printf("Informe o b: ");
    scanf("%d", &b);

    soma = a + b;
    sub = a - b;
    multi = a * b;
    divi = a / b;
    resto = a % b;

    printf("Resultado da adicao: %d\n", soma);
    printf("Resultado da subtracao: %d\n", sub);
    printf("Resultado da multiplicacao: %d\n", multi);
    printf("Resultado da divisao: %d\n", divi);
    printf("Resultado do resto: %d\n", resto);
}
    
